{
  "name": "obsidian-plugin-template",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@9.5.0",
  "description": "Obsidian Template Plugin",
  "author": {
    "name": "Nólëbase",
    "email": "nolebase@ayaka.moe",
    "url": "https://github.com/nolebase"
  },
  "license": "MIT",
  "keywords": [
    "nolebase",
    "readabilities",
